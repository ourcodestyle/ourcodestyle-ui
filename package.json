{
  "name": "ourcodestyle",
  "version": "0.1.0",
  "description": "Argue, Discuss, Compare - Consensus!",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run open:src\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "__prebuild": "npm run lint && npm run test && npm run clean-dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "deploy": "rm -rf ./dist/* && npm run dist && scp -r dist/* ourcodestyle.com:./ourcodestyle/ui",
    "dist": "babel-node tools/build.js",
    "build-open:dist": "npm run dist && npm run open:dist",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Vitaliy Yanchuk",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/polyfill": "7.7.0",
    "@babel/preset-react": "7.7.4",
    "@blueprintjs/core": "3.23.1",
    "@blueprintjs/icons": "3.13.0",
    "@blueprintjs/select": "3.11.2",
    "actioncable": "5.1.5",
    "apollo-cache-inmemory": "1.6.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.2",
    "apollo-link-context": "1.0.8",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "1.5.4",
    "apollo-link-ws": "1.0.9",
    "apollo-upload-client": "8.0.0",
    "babel-plugin-jsx-control-statements": "4.0.0",
    "babel-plugin-wildcard": "5.0.0",
    "classnames": "2.2.6",
    "codemirror": "5.40.0",
    "connected-react-router": "6.6.1",
    "copy-to-clipboard": "3.0.8",
    "copy-webpack-plugin": "4.5.1",
    "date-fns": "1.30.1",
    "eslint-import-resolver-babel-plugin-root-import": "0.0.11",
    "extract-files": "3.1.0",
    "graphql": "0.13.2",
    "graphql-ruby-client": "1.4.1",
    "graphql-tag": "2.8.0",
    "http-proxy-middleware": "0.18.0",
    "humps": "2.0.1",
    "immutable": "3.8.2",
    "lodash": "4.17.13",
    "node-pre-gyp": "0.14.0",
    "normalize.css": "8.0.0",
    "object-assign": "4.1.0",
    "object-hash": "1.3.0",
    "pluralize": "7.0.0",
    "query-string": "5.1.0",
    "ramda": "0.25.0",
    "react": "^16.12.0",
    "react-apollo": "2.1.3",
    "react-codemirror2": "5.1.0",
    "react-dom": "^16.12.0",
    "react-markdown": "4.0.8",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.1.2",
    "react-timeago": "4.3.0",
    "react-transition-group": "2.2.1",
    "redux": "3.6.0",
    "redux-saga": "0.16.0",
    "slugify": "1.3.4",
    "subscriptions-transport-ws": "0.9.15",
    "uuid": "3.2.1",
    "yaml": "1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@playlyfe/gql": "2.6.2",
    "autoprefixer": "7.1.4",
    "babel-eslint": "7.2.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-root-import": "5.1.0",
    "browser-sync": "2.18.12",
    "chalk": "2.0.1",
    "concurrently": "3.5.0",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.13.1",
    "css-loader": "0.28.7",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "eslint": "4.18.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-watch": "3.1.2",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "0.11.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "json-loader": "0.5.4",
    "mockdate": "2.0.1",
    "node-sass": "4.12.0",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.6",
    "prompt": "1.0.0",
    "prop-types": "15.5.10",
    "raf": "3.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "16.0.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.3.0",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.5.9",
    "webpack": "3.1.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "babel": {
    "plugins": [
      "wildcard",
      "jsx-control-statements",
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "src"
        }
      ]
    ],
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      },
      "production": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": []
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jsx-control-statements/recommended"
    ],
    "plugins": [
      "react",
      "jsx-control-statements"
    ],
    "settings": {
      "import/resolver": "babel-plugin-root-import"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "rules": {
      "quotes": 0,
      "no-console": 1,
      "no-debugger": 1,
      "no-var": 1,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0,
      "jsx-quotes": 1,
      "react/display-name": [
        1,
        {
          "ignoreTranspilerName": false
        }
      ],
      "react/forbid-prop-types": [
        1,
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-curly-spacing": 1,
      "react/jsx-indent-props": 0,
      "react/jsx-key": 1,
      "react/jsx-max-props-per-line": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-no-duplicate-props": 1,
      "react/jsx-no-literals": 0,
      "react/jsx-no-undef": 1,
      "react/jsx-pascal-case": 1,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/jsx-wrap-multilines": 1,
      "react/no-danger": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 1,
      "react/no-multi-comp": 1,
      "react/no-set-state": 0,
      "react/no-unknown-property": 1,
      "react/prefer-es6-class": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "import/extensions": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1
    },
    "globals": {}
  }
}
